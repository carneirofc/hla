    <scripts>
      <path pathString="EmbeddedPy" checkConnect="true" sfe="false" seoe="false">
        <scriptName>EmbeddedScript</scriptName>
        <scriptText><![CDATA[from org.csstudio.opibuilder.scriptUtil import PVUtil

pv0 = PVUtil.getString(pvs[0])
pv1 = PVUtil.getDouble(pvs[1])
sc = 1e6
if pv0 == "ms":
	neg_lim = pv1 - 0.5*sc
	if neg_lim <0: neg_lim = 0.0
	widget.setPropertyValue("maximum",pv1 + 0.5*sc)
	widget.setPropertyValue("minimum", neg_lim)
	widget.setPropertyValue("page_increment",10e-3*sc)
	widget.setPropertyValue("step_increment",1e-3*sc)
	widget.setPropertyValue("bar_length",1e-3*sc)
elif pv0 == "us":
	neg_lim = pv1 - 0.5e-3*sc
	if neg_lim <0: neg_lim =0.0
	widget.setPropertyValue("maximum",pv1 + 0.5e-3*sc)
	widget.setPropertyValue("minimum", neg_lim)
	widget.setPropertyValue("page_increment",10e-6*sc)
	widget.setPropertyValue("step_increment",1e-6*sc)
	widget.setPropertyValue("bar_length",1e-6*sc)
elif pv0 == "ns":
	neg_lim = pv1 - 0.5e-6*sc
	if neg_lim <0: neg_lim =0.0
	widget.setPropertyValue("maximum",pv1 + 0.5e-6*sc)
	widget.setPropertyValue("minimum", neg_lim)
	widget.setPropertyValue("page_increment",10e-9*sc)
	widget.setPropertyValue("step_increment",1e-9*sc)
	widget.setPropertyValue("bar_length",1e-9*sc)]]></scriptText>
        <pv trig="true">loc://scale("ms")</pv>
        <pv trig="false">loc://test(0)</pv>
      </path>
