            <scriptName>EmbeddedScript</scriptName>
            <scriptText><![CDATA[from org.csstudio.opibuilder.scriptUtil import PVUtil, ConsoleUtil

pv0 = PVUtil.getString(pvs[0])
pv1 = PVUtil.getDouble(pvs[1])

props = {
	"1ns": 1e-3,
	"0.1us": 1e-1,
	"10us": 1e1,
	"1ms": 1e3,
	"0.1s": 1e5,
	}

sc = props[str(pv0)]

neg_lim = pv1 - 100*sc
if neg_lim <0: neg_lim = 0.0
widget.setPropertyValue("maximum",pv1 + 100*sc)
widget.setPropertyValue("minimum", neg_lim)
widget.setPropertyValue("page_increment",10*sc)
widget.setPropertyValue("step_increment",1*sc)
widget.setPropertyValue("bar_length",1*sc)

]]></scriptText>
            <pv trig="true">loc://$(P)$(Evnt)Scale("1ns")</pv>
            <pv trig="false">$(pv_name)</pv>
          </path>
